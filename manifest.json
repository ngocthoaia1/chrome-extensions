{
  "name": "Chrome extensions",
  "version": "1.0",
  "description": "This extensions developed by NNT.",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*","http://*/*"],
      "js": [
        "js/jquery-2.1.3.js",
        "js/hide-iframe.js",
        "js/hide-elements.js"
      ],
      "css": ["css/hide-iframe.css"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "manifest_version": 2
}
